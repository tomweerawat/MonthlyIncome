        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".utility.BaseActivity" />
        <activity android:name=".activity.java.MainBackup" />
        <activity android:name=".activity.java.DatePickerActivity"/>
        <activity android:name=".activity.ActivityChoose"/>
        <activity android:name=".activity.HomeMenu"/>
        <activity android:name=".activity.ActivityNewCus"/>
        <activity android:name=".fragment.FragmentCust"/>
        <activity android:name=".activity.ActivityProfile"/>
        <activity android:name=".activity.MainActivity"/>
    </application>

</manifest>