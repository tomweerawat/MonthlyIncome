<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    card_view:cardCornerRadius="6dp"
    card_view:cardElevation="3dp"
    card_view:cardUseCompatPadding="true">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical"
        android:padding="5dp">


        <!-- Text Notice Name -->
        <TextView
            android:id="@+id/startDate1"
            android:layout_width="match_parent"
            android:textSize="26sp"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:padding="3dp"/>

        <!-- Text Notice Description -->
        <TextView
            android:id="@+id/endDate1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp" />

        <!-- Text Notice FilePath -->
        <TextView
            android:id="@+id/Merchant1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:padding="3dp"/>
        <!-- Text Notice Description -->
        <TextView
            android:id="@+id/Service1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp" />

        <!-- Text Notice FilePath -->
        <TextView
            android:id="@+id/UnitTime1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:padding="3dp"/>
        <TextView
            android:id="@+id/VATCase1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:padding="3dp"/>

        <TextView
            android:id="@+id/MerchantIncome1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/ServiceValue1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/StringVAT1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/CommissionfromMerchant1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/MerchantNetIncome1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/FEE1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/FEEDiscountfromcode1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/Discountfromcode1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />
    <TextView
            android:id="@+id/DiscountfromCoin1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/TotalDiscount1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/Nameoftax1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/TaxNo1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/Bank1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/Bankaccountno1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />


        <TextView
        android:id="@+id/name1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:textSize="12sp" />

        <TextView
            android:id="@+id/Bankbanch1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/billaddress1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/Mail1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:textSize="12sp" />
    </LinearLayout>

</android.support.v7.widget.CardView>